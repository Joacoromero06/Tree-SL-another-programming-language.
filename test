
# pyright: reportGeneralTypeIssues=false
# type: ignore
# mypy: disable-error-code
 

function fibo(n):
    if(n >= 2)
        return fibo(n-1) + fibo(n-2);        
    else 
        if (n == 1)
            return 1;
        else 
            return 0;
        endif
    endif
end
main:
    forall (x in [0,1,2,3,4,5,6,7,8]) do 
        println fibo(x); 
    end
endmain
